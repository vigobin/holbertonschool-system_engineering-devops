#!/usr/bin/env bash
# Script that gives you information about the school file

file="school"
if [ -e "$file" ]; then
echo "$file file exists"
else
echo "$file file does not exist"
fi
if [ ! -s "$file" ]; then
 echo "$file file is empty"
else
echo "$file file is not empty"	
fi
if [ -f "$file" ]; then
echo "$file is a regular file"
else
echo ""
fi
